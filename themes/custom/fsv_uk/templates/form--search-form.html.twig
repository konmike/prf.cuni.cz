{#
/**
 * @file
 * Theme override for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 */
#}

<div class="search-results__top mb">
    
    <h2 class="section-title padding">{{ 'Search'|t }}</h2>
    <form{{ attributes.addClass('search-results__form padding padding--large') }}>
      {{ children }}
    </form>

</div>
